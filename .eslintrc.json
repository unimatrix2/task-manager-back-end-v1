{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "no-restricted-syntax": "off",
        "indent": ["error", 4]
    }
}
